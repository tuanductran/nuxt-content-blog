{
  "name": "nuxt-content-blog",
  "version": "1.0.0",
  "description": "A modern, user-friendly blog built with Nuxt Content and optimized for performance",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuanductran/nuxt-content-blog.git"
  },
  "bugs": {
    "url": "https://github.com/tuanductran/nuxt-content-blog/issues"
  },
  "author": "Tuan Duc Tran <tuanductran.dev@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.6.3",
  "keywords": [
    "nuxt",
    "content",
    "blog",
    "vue",
    "ssg",
    "markdown"
  ],
  "scripts": {
    "build": "nuxi build",
    "check:md": "node ./scripts/check-md.mjs",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:text": "case-police",
    "lint:text:fix": "case-police --fix",
    "postinstall": "nuxi prepare || husky",
    "preinstall": "npx only-allow pnpm",
    "prettier": "concurrently \"pnpm lint:fix\" \"pnpm lint:text:fix\" \"pnpm lint:prettier:fix\"",
    "preview": "nuxi preview",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@nuxt/image": "^1.9.0",
    "@nuxt/ui": "^3.0.0",
    "@vueuse/core": "^13.0.0",
    "@vueuse/nuxt": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@iconify-json/devicon": "^1.2.19",
    "@iconify-json/solar": "^1.2.2",
    "@nuxt/content": "^3.3.0",
    "@nuxt/devtools": "^2.3.0",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/icon": "^1.11.0",
    "@nuxthub/core": "0.8.18",
    "case-police": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "markdownlint-cli": "^0.44.0",
    "nuxt": "^3.16.0",
    "prettier": "^3.5.3",
    "prettier-plugin-pkg": "^0.18.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.2",
    "vue-tsc": "^2.2.8",
    "wrangler": "^3.114.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
