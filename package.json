{
  "name": "nuxt-content-blog",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "nuxi build",
    "check:md": "node ./scripts/check-md.mjs",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --write",
    "lint:text": "case-police",
    "lint:text:fix": "case-police --fix",
    "postinstall": "nuxi prepare || husky",
    "prettier": "concurrently \"pnpm lint:fix\" \"pnpm lint:text:fix\" \"pnpm lint:prettier:fix\"",
    "preview": "nuxi preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/image": "^1.9.0",
    "@nuxt/ui": "^2.21.0",
    "@nuxthub/core": "^0.8.18",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@iconify-json/devicon": "^1.2.18",
    "@iconify-json/solar": "^1.2.2",
    "@nuxt/content": "^3.2.2",
    "@nuxt/devtools": "^2.1.1",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/icon": "^1.10.3",
    "@nuxtjs/fontaine": "^0.5.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "case-police": "^1.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "markdownlint-cli": "^0.44.0",
    "nuxt": "^3.15.4",
    "prettier": "^3.5.3",
    "prettier-plugin-pkg": "^0.18.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.3",
    "vue-tsc": "^2.2.8",
    "wrangler": "^3.111.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
